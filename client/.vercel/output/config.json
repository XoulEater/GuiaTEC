{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/activity\\/edit-([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/forgot-password\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/new-activity\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/new-password\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/register\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/register-success\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/students\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/teacher\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/teachers\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/team\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/validate-email\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/workplan\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		}
	]
}